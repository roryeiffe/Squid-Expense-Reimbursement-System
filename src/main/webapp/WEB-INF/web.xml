<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>reimbursementRequest</servlet-name>
    <servlet-class>RequestServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>viewRequest</servlet-name>
    <servlet-class>ViewRequestServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>reimbursementRequest</servlet-name>
    <url-pattern>/request</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>viewRequest</servlet-name>
    <url-pattern>/viewRequest</url-pattern>
  </servlet-mapping>


</web-app>
